@using Orchard.ContentManagement;

@{
    var content = (IContent)Model.ContentPart;
    var alternateText = content.As<Orchard.MediaLibrary.Models.MediaPart>().AlternateText;
}

@Display.CloudVideoPlayer(
    ContentPart: Model.ContentPart,
    ContentItem: Model.ContentPart.ContentItem,
    AlternateText: alternateText,
    AssetId: null,
    IgnoreIncludeInPlayer: false,
    AllowPrivateUrls: false,
    PlayerWidth: 600,
    PlayerHeight: 400,
    ApplyMediaQueries: true,
    AutoPlay: false
)